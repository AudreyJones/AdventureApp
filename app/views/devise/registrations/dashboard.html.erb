require 'pry'
<h1>Welcome, <%=current_user.first_name%> <%=current_user.last_name%></h1>

<h3>Your Current Adventures:</h3>
<ol>
    <%current_user.adventures.each do |adventure|%>
    
        <li><%= link_to(adventure.name, adventure_path(adventure)) %> - <%= adventure.location %></li>
    <%end%>
</ol>